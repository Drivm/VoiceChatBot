{
    "name": "my first app",
    "description": "cute app.",
    "keywords": ["app"],
    "repository": "https://github.com/Drivm/VoiceChatBot",
  "stack": "container",
    "env": {
        "STRING_SESSION": {
            "description": "Telethon String Session",
            "required": true
        },
        "HEROKU_MODE": {
            "description": "Enable to disable many features",
            "required": true,
            "value": "ENABLE"
        },
        "API_ID": {
            "description": "your Api ID from my.telegram.org/apps",
            "required": true,
            "value": ""
        },
        "API_HASH": {
            "description": "your Api Hash from my.telegram.org/apps",
            "required": false,
            "value": ""
        },
        "SUPPORT": {
            "description": "",
            "required": false,
            "value": "ivbotX"
        },
        "CHANNEL": {
            "description": "",
            "required": false,
            "value": "ivbotX"
        },
        "BOT_USERNAME": {
            "description": "Fill there your bot username",
            "required": false,
            "value": "ivribot"
        },
        "BOT_TOKEN": {
            "description": "bot token. get from @BotFather on telegram",
            "required": true,
            "value": ""
        }

    }
  }
